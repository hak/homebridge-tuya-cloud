{
  "pluginAlias": "TuyaCloudHomebridgePlugin",
  "pluginType": "platform",
  "singular": true,
  "schema": {
    "type": "object",
    "properties": {
      "url": {
        "title": "Region",
        "type": "string",
        "required": true,
        "default": "https://openapi.tuyaus.com",
        "oneOf": [
          {
            "title": "USA",
            "enum": ["https://openapi.tuyaus.com"]
          },
          {
            "title": "Europe",
            "enum": ["https://openapi.tuyaeu.com"]
          },
          {
            "title": "India",
            "enum": ["https://openapi.tuyain.com"]
          },
          {
            "title": "China",
            "enum": ["https://openapi.tuyacn.com"]
          }
        ]
      },
      "clientID": {
        "title": "clientID",
        "type": "string",
        "required": true,
        "default": ""
      },
      "secret": {
        "title": "secret",
        "type": "string",
        "required": true,
        "default": ""
      }
    }
  }
}
